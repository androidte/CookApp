<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zt8989.cookapp" >

    <uses-sdk android:minSdkVersion="15" android:targetSdkVersion="22" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SubListActivity">

        </activity>
        <activity android:name=".CookListActivity"></activity>
        <activity android:name=".CookDetailActivity"></activity>
        <activity android:name=".SearchListActiviy">
            <intent-filter>
                <action android:name="com.zt8989.CookApp.ACTION_SEARCH" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="com.zt8989.CookApp.RESULT" />
                <category android:name="com.zt8989.CookApp.SEARCH" />
            </intent-filter>
        </activity>
    </application>

</manifest>
